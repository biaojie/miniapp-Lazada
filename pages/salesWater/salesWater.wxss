/* pages/salesWater/salesWater.wxss */
page{
  font-size: 32rpx;
  font-family: '微软雅黑';
  background-color: #f5f5f9;
}
image{
  width: 100%;
  height: 100%;
}
.top{
  width: 100%;
  background: linear-gradient(to top, #F4F4F8 50%, #009CFF 50%);
  display: flex;
  justify-content: center;
  padding-top: 20rpx;
}
.topinfo{
  width: 95%;
  border: 2rpx solid #D8D8D8;
  border-radius: 10rpx;
  background-color: #FFF; 
  padding: 30rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
  display: flex;
}
.topinfo1{
  width: 50%;
  display: flex;
  flex-direction: column;
}
.topinfo1 image{
  width:120rpx;
  height:120rpx;
  border-radius:180rpx;
  border:2rpx solid #D8D8D8;
  margin: auto;
}
.topinfo1 text{
  width: 100%;
  text-align: center;
  padding-top: 20rpx;
}
#price{
  color: #E51C23;
  font-size: 38rpx;
  font-weight: 700;
  margin: auto;
}
#border{
  border-right: 2rpx dotted #D8D8D8;
}
.navall{
  width: 100%;
  display: flex;
  margin-top: 30rpx;
  background-color: #FFF;
}
.naved{
  width: 50%;
  text-align: center;
  padding: 25rpx;
  border-bottom: 6rpx solid #009CFF;
}
.nav{
  width: 50%;
  text-align: center;
  padding: 25rpx;
  border-bottom: 2rpx solid #D8D8D8;
}
.times{
  width: 100%;
  display: flex;
  padding: 25rpx;
  box-sizing: border-box;
  font-weight: bold;
}
.timed{
  color: #FE4081;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
}
.times image{
  width: 40rpx;
  height: 40rpx;
}
.time{
  padding: 0 30rpx;
  display: flex;
  align-items: center;
}
.sw{
  width: 100%;
  padding: 25rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  background-color: #FFF;
}
.table{
  width: 100%;
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
  border: 1rpx solid #D8D8D8;
}
.tr{
  width: 100%;
  display: flex;
}
.ty{
  width: 100%;
  display: flex;
  justify-content: flex-end;
}
.ty text{
  padding: 25rpx;
}
.th{
  font-weight: 700;
}
.td{
  flex: 1;
  text-align: center;
  border: 1rpx solid #D8D8D8;
  padding: 20rpx 0;
  overflow: hidden;
  max-height: 70rpx;
  min-height: 40rpx;
}
.tz{
  color: #E51C23;
  padding: 20rpx 0;
   overflow: hidden;
}
.jsall{
  background-color: #FFF;
  padding: 20rpx;
  box-sizing: border-box
}
.js{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  background-color: #38C3FF;
  border-radius: 5rpx;
  padding: 25rpx 0;
}
.js2{
  width: 100%;
  color: #FFF;
  padding-bottom: 25rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
}
.js1{
  width: 50%;
  display: flex;
  flex-direction: column;
  text-align: center;
  box-sizing: border-box;
}
#js11{
  color: #FFF;
  font-weight: 700;
  font-size: 38rpx;
  padding-bottom: 15rpx;
}
#right{
  border-right: 2rpx solid #FFF;
}
.an{
  width: 100%;
  box-sizing: border-box;
  padding: 25rpx;
  background-color: #F5F5F9;
  margin-top: 10rpx;
  border-bottom: 2rpx solid #D8D8D8;
}
.an1{
  width: 100%;
  box-sizing: border-box;
  padding: 25rpx;
  background-color: #FFF;
  border-bottom: 2rpx solid #D8D8D8;
  display: flex;
  font-weight: 700;
  color: #E51C23;
  align-items: center;
}
.an1 text{
  color: #000;
  font-weight: normal;
  flex: 1;
}
.an1 image{
  width: 40rpx;
  height: 40rpx;
  margin-left: 20rpx;
}